{
  "schemaVersion": 1,
  "id": "item-frame-dupe",
  "version": "${version}",
  "name": "Dupe Plugin",
  "description": "A plugin for duping",
  "authors": [
    "opfromthestart"
  ],
  "contact": {
    "repo": "https://github.com/opfromthestart/item-frame-dupe"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "org.gmail.opfromthestart.dupes.DupeAddon"
    ]
  },
  "mixins": [
    "template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
